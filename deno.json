{
  "compilerOptions": {
    "lib": [
      "deno.window",
      "deno.ns"
    ],
    "strict": true
  },
  "importMap": "./import_map.json",
  "tasks": {
    "start": "deno run --allow-net --allow-read --allow-write --allow-env --allow-run app.ts"
  },
  "permissions": {
    "net": true,
    "read": true,
    "write": true,
    "env": true,
    "run": true
  }
}